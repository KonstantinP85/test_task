<div id="result-action" class="container-fluid mt-3">
    {% if roles is defined %}
    <table class="table table-dark table-striped table-hover">
        <thead>
        <tr>
            <th scope="col">Объекты</th>
            {% for roles_item in roles %}
                <th scope="col">{{ roles_item.name }}</th>
            {% endfor %}
        </tr>
        </thead>
        <tbody>
        {% for objectClasses_item in objectClasses %}
        <tr>
            <td>{{ objectClasses_item.name }}</td>
            {% for roles_item in roles %}
            <td>
                <input class="form-check-input" type="checkbox" value="" id="flexCheckDefault">
            </td>
            {% endfor %}
        </tr>
        {% endfor %}
        </tbody>
    </table>
    {% endif %}
</div>